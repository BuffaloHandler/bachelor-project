func add(l:List, n:ListNode) : int
  if(list.head == null)
  then{
    list.head = n;
    list.tail = n;
    empty = false;
  }
  else{
    n.next = list.head;
    n.next.prev = n;
    n.prev = null;
    l.head = n;
  }
  return 0;
end add

func removeLast(l:List) : ListNode
  var n:ListNode;
  if(list == null || list.tail == null)
    then return null;
  else{
    l.tail = n.prev;
    l.tail.next = null;
    n.next = null;
    n.prev = null;
    if(l.tail == null)
    then {
      l.head = null;
      empty = true;
    }
    return n;
  }
end removeLast

func noArgs(): int
  return 0;
end noArgs

