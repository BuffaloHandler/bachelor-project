func realFib(tal:int):int
  if(tal == 0) then
    return 0;
  else
    write 7;
  return realFib(tal-1) + realFib(tal-2);
end realFib


func fib(n:int):int
  #return fib(n+1)+fib(n+2);
  #n=n+1;
  var x:int;
  x = f(n,n+10)+f(n+2,n+3);
  return x;
end fib

func f(a:int, b:int):int
  return a-b;
end f

