#Tildeling forkert mellem elementer i arrays (simple typer) (FORBUDT)
var a : array of A;
var b : array of bool;
var i:int, j:int, k:int;
type A = array of array of int;
allocate a of length 5;
i = 0;
while(i < 5) do{
  allocate a[i]  of length 4;
  j = 0;
  while(j < 4) do{
    allocate a[i][j] of length 3;
    k = 0;
    while(k < 3) do{
      a[i][j][k] = i*100+j*10+k;
      k = k + 1;
    }
    j = j + 1;
  }
  i = i + 1;
}

allocate b of length 3;
i = 0;
while(i < 3) do{
  b[i] = true;
  i = i + 1;
}

a[0][0][0] = b[1];
a[2][1][1] = b[2];
b[2] = a[4][1][1];
b[0] = a[3][2][1];
