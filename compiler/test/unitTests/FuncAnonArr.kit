  funktioner der tager og returnere anonyme arrays
  korrekt kitty program*)

func retAnonArray (x : array of array of int) : array of array of int

	var v : array of array of int;
	var i : int, j : int;
	i = 0;
	allocate v of length 5;
	while (i < 5) do {
		allocate v[i] of length 5;
		j = 0;
		while(j<5) do {
			v[i][j] = x[4-i][4-j];
			j = j + 1;
		}
		i = i + 1;
	}
	return v;

end retAnonArray

var q : array of array of int;
var t : array of array of int;
var i : int, j : int;

allocate q of length 5;
i = 0;
while (i < 5) do {
	allocate q[i] of length 5;
	j = 0;
	while(j<5) do {
		q[i][j] = i + j;
		j = j + 1;
	}
	i = i + 1;
}

t = retAnonArray(q);

i = 0;
while (i < 5) do {
	j = 0;
	while(j<5) do {
		write t[i][j];
		j = j + 1;
	}
	i = i + 1;
}

