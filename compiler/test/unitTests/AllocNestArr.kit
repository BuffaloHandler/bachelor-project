var a: array of A;
type A = array of array of int;
var i:int, j:int, k:int;
allocate a of length 3;
i = 0;
while(i < 3) do{
  allocate a[i] of length (i+1);
  i = i + 1;
}

i = 0;
while(i < 3) do{
  j = 0;
  while(j <= i) do{
    allocate a[i][j] of length (j+1);
    j = j + 1;
  }
  i = i + 1;
}
