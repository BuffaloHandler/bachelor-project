func pow (number : int, power : int) : int
	
	if (power > 1) then {
		number = number * 2;
		return pow(number, power-1);
	}
	return number;
	
end pow

func remainder (divident : int, divisor : int) : int

	while (divident >= divisor) do {
		divident = divident - divisor;
	}
	return divident;

end remainder

func whileCondition () : bool

	var x : int;
	x = 100;
	while (x > 2) do {
		x = x - 1;
	}
	return true;

end whileCondition
