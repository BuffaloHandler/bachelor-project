#tildeling mellem arrays (b√•de simple og navngivne typer)
var a : array of A;
var b : A;
var c : array of int;
var i:int, j:int, k:int;
type A = array of int;


allocate a of length 4;

i = 0;

while(i < 4) do{
  allocate a[i]  of length 3; #problems start here
  j = 0;
  while(j < 3) do{
    a[i][j] = i*10+j;
    j = j + 1;
  }
  i = i + 1;
}

allocate b of length 8;
i = 0;
while(i < 8) do{
  b[i] = i;
  i = i + 1;
}

c = b;
b[6] = 34;
a[1] = c;


i = 0;
while(i < 4) do{
  j = 0;
  while(j < |a[i]|) do{
    write a[i][j];
    j = j + 1;
  }
  i = i + 1;
}

i = 0;
while(i < 8) do{
  write b[i];
  i = i + 1;
}

i = 0;
while(i < |c|) do{
  j = 0;
  write c[i];
  i = i + 1;
}

