type myInt = int;
type myBool = bool;

func retInt (x : myInt) : myInt

	x = x + 1;
	return true;

end retInt

func retBool (boolean : myBool) : myBool

	boolean = false;
	return 2;

end retBool

var x : myInt, y:myBool;

x = 42;
y = true;

write retInt(x);
write retBool(y);