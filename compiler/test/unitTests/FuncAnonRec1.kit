func retAnonRec (vector : record of {x:int, y:int, z:int}) : int
	var w : record of {x:int, y:int, z:int};

	allocate w;
	w.x = vector.x;
	w.y = vector.y;
	w.z = vector.z;
	return w.x;

end retAnonRec

var v : record of {x:int, y:int, z:int};
var u : int;

allocate v;
v.x = 13;
v.y = 3;
v.z = 7;

u = retAnonRec(v);

write u;
