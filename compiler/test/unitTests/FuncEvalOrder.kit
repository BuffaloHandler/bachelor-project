var globalInt : int;

func multiplyBy10() : int

	globalInt = globalInt * 10;
	return globalInt;

end multiplyBy10

func decreseBy10() : int

	globalInt = globalInt - 10;
	return globalInt;

end decreseBy10

func divideThings(x : int, y : int) : int
	return x / y;
end divideThings

