(*Declaration list*)
type Node = record of {
  data : int ,
  leaf : bool,
  left : Node,
  right : Node
};

func trav (n : Node): int
  if n.leaf == false
  then
    return n.data;
  else
    return trav(n.right);
end trav

var node : Node;


